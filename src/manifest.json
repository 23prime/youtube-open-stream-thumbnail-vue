{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "homepage_url": "https://github.com/23prime/youtube-open-stream-thumbnail-vue",
    "description": "Open the thumbnail of current YouTube stream in new tab.",
    "default_locale": "en",
    "permissions": ["tabs", "<all_urls>", "*://*/*"],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "__MSG_extName__",
        "default_icon": {
            "19": "icons/19.png",
            "38": "icons/38.png"
        }
    }
}
